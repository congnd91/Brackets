/*
 * This is a read-only file with the preferences supported
 * by Brackets.
 * Use this file as a reference to modify your preferences
 * file "brackets.json" opened in the other pane.
 * For more information on how to use preferences inside
 * Brackets, refer to the web page at https://github.com/adobe/brackets/wiki/How-to-Use-Brackets#preferences
 */

{
	// External Beautifiers, see https://github.com/brackets-beautify/brackets-beautify#external-formatters
	"bb.beautify.beautifiers": {},

	// true to hide the dialog warning about unsupported languages
	"bb.beautify.hideDialog": false,

	// Beautifier-Language-Mapping, see https://github.com/brackets-beautify/brackets-beautify#configure-languages
	"bb.beautify.languages": {
		// Default: css
		"css": "css",

		// Default: html
		"ejs": "html",

		// Default: html
		"handlebars": "html",

		// Default: html
		"html": "html",

		// Default: js
		"javascript": "js",

		// Default: js
		"json": "js",

		// Default: js
		"jsx": "js",

		// Default: css
		"less": "css",

		// Default: html
		"php": "html",

		// Default: css
		"scss": "css",

		// Default: html
		"svg": "html",

		// Default: html
		"vue": "html",

		// Default: html
		"xml": "html"
	},

	// true to Beautify file on Save
	"bb.beautify.onSave": false,

	// Default: true
	"brackets-eslint.gutterMarks": true,

	// Default: false
	"brackets-eslint.useLocalESLint": false,

	"brackets-icons.icons": {},

	// Default: ionicons
	"brackets-icons.iconset": "ionicons",

	// Default: true
	"brackets-icons.secondary": true,

	// Determines whether the indent guides will be shown or hidden.
	"brackets-indent-guides.enabled": false,

	// The color of the indent guides. Can be any valid CSS Color value.
	"brackets-indent-guides.guideColor": "rgba(128, 128, 128, 0.5)",

	// This value determines the style of the guide: "solid" or "dotted".
	"brackets-indent-guides.guideStyle": "solid",

	// Determines whether the first Indent Guide will be hidden or shown.
	"brackets-indent-guides.hideFirst": false,

	// Default: 
	"brackets-snippets.showSnippetsPanelShortcut": "",

	// Default: data
	"brackets-snippets.snippetsDirectory": "data",

	// Default: Ctrl-Alt-Space
	"brackets-snippets.triggerSnippetShortcut": "Ctrl-Alt-Space",

	// true to automatically close braces, brackets and parentheses
	"closeBrackets": true,

	// false to remove the "Close Others Above" from the Working Files context menu
	"closeOthers.above": true,

	// false to remove the "Close Others Below" from the Working Files context menu
	"closeOthers.below": true,

	// false to remove the "Close Others" from the Working Files context menu
	"closeOthers.others": true,

	// Sets the tag closing options
	"closeTags": {
		// An array of tags that should not be auto-closed
		"dontCloseTags": [],

		// An array of tags that when opened has a blank line
		"indentTags": [],

		// Close when / of closing tag is typed
		"whenClosing": true,

		// Close when > of opening tag is typed
		"whenOpening": true
	},

	// true to always generate collapsible section markers when indent level changes
	"code-folding.alwaysUseIndentFold": false,

	// true to enable code folding
	"code-folding.enabled": true,

	// true to make Section-collapsing markers visible only when you move the mouse over the gutter
	"code-folding.hideUntilMouseover": false,

	// true to enable code folding on selected text in the editor
	"code-folding.makeSelectionsFoldable": true,

	// Limits how many levels deep Collapse All applies
	"code-folding.maxFoldLevel": 2,

	// Minimum lines before a collapsible section icon appears
	"code-folding.minFoldSize": 2,

	// true to remember collapsed sections if you close and reopen a file or project
	"code-folding.saveFoldStates": true,

	// Enable/disable HTML attribute hints
	"codehint.AttrHints": true,

	// Enable/disable CSS/LESS/SCSS property hints
	"codehint.CssPropHints": true,

	// Enable/disable JavaScript code hints
	"codehint.JSHints": true,

	// Enable/disable Preferences code hints
	"codehint.PrefHints": true,

	// Enable/disable SVG code hints
	"codehint.SVGHints": true,

	// Enable/disable HTML entity hints
	"codehint.SpecialCharHints": true,

	// Enable/disable HTML tag hints
	"codehint.TagHints": true,

	// Enable/disable URL hints in HTML & CSS/LESS/SCSS
	"codehint.UrlCodeHints": true,

	// Hidden the box(tabMenu) of the right hand if visible, this happens when the document in focus changes their state.
	"custom.work.autoHideTheTabmenu": false,

	// Apply a bluish style to the tab bar
	"custom.work.blueish": false,

	// Mode Icons Grayscale, only Icons of 'Custom Work'. non included when moves tabs.
	"custom.work.grayScaleIcons": false,

	// Editor Gutter Width - Ability to toggle/retrive bookmarks by clicking on gutter. a vertical bar in right side of the editor with indicators of bookmarks in the current editor.
	"custom.work.gutter.bookmark": false,

	// Moves all buttons of toolbar to bottom sidebar.
	"custom.work.moveToolbarToSidebar": false,

	// Reduce the opacity of the non-focused pane.
	"custom.work.reduce.opacity": false,

	// If false, change the tabs width automatically based on the size of the editor.
	"custom.work.scrollwheel": false,

	// If navigation by scrolling is enabled, this option will automatically display the file in the tab menu(top horizontal tab).
	"custom.work.scrollwheel.change": false,

	// If previous is true (scroll wheel), this will advance n pixels to the right or left according to the rotation of the mouse wheel.
	"custom.work.scrollwheel.step": 50,

	// Show default working listview on left Sidebar.
	"custom.work.showWorkingListViewonLeftSidebar": false,

	// If checked; displays a one or two buttons on each tabs. This makes it possible to switch between the editors the current document.
	"custom.work.splitButton": false,

	// Shows a Tooltip with information about the File.
	"custom.work.tooltip": false,

	// Use icons of this addon (CUSTOM WORK) instead of icons of other addons.
	"custom.work.useIconsOfCustomWork": false,

	// true to show errors in status bar
	"debug.showErrorsInStatusBar": false,

	// 
	"dimitrk.exclude-indexing-filetree.excludeList": [],

	// Enable/disable Drag & Drop functionality
	"dragDropText": false,

	// Default extensions that are disabled
	"extensions.default.disabled": [],

	// Sort Extensions using downloadCount or publishedDate
	"extensions.sort": "publishedDate",

	// true to enable instant search
	"findInFiles.instantSearch": true,

	// true to enable node based search
	"findInFiles.nodeSearch": true,

	// Change font family
	"fonts.fontFamily": "'SourceCodePro-Medium', ＭＳ ゴシック, 'MS Gothic', monospace",

	// Change font size; e.g. 13px
	"fonts.fontSize": "12px",

	// Enable the Health Data Tracking
	"healthData.healthDataTracking": true,

	// Enables automatic highlighting of matching strings throughout the document
	"highlightMatches": false,

	// true to enable indenting of line comments
	"indentLineComment": false,

	// Selects the way CodeMirror handles input and focus. It can be textarea, which is the default, or contenteditable which is better for screen readers (unstable)
	"inputStyle": "textarea",

	// true to insert the currently selected code hint on tab
	"insertHintOnTab": false,

	// Default: false
	"insya-module-highlighter.enabled": false,

	// A list of files that have been detected to cause Tern to run out of control
	"jscodehints.detectedExclusions": [],

	// The amount of time after which Tern will time out when trying to understand files
	"jscodehints.inferenceTimeout": 30000,

	// true to not automatically show JS code hints when . is typed
	"jscodehints.noHintsOnDot": false,

	// Enable/disable datatype details in JavaScript code hints
	"jscodehints.typedetails": true,

	// An object with the default options for JSLint
	"jslint.options": {
		// true to allow assignment expressions
		"ass": false,

		// true to allow bitwise operators
		"bitwise": false,

		// true if standard browser globals should be predefined
		"browser": false,

		// true to allow Google Closure idioms
		"closure": false,

		// true to allow "continue" statement
		"continue": false,

		// true if CouchDB globals should be predefined
		"couch": false,

		// true to allow debugger statements
		"debug": false,

		// true if browser globals useful in development should be predefined
		"devel": false,

		// true to allow == and !=
		"eqeq": false,

		// true if ES6 globals should be predefined
		"es6": false,

		// true to allow eval
		"evil": false,

		// true to allow unfiltered "for ... in"
		"forin": false,

		// Set a specific tab width
		"indent": 0,

		// The maximum number of warnings reported
		"maxerr": 0,

		// The maximum number of characters in a line
		"maxlen": 0,

		// true to allow uncapitalized constructors
		"newcap": false,

		// true if Node.js globals should be predefined
		"node": false,

		// true to allow dangling underscore in identifiers
		"nomen": false,

		// true to stop on first error
		"passfail": false,

		// true to allow ++ and --
		"plusplus": false,

		// true to allow . and [^...]. in RegExp
		"regexp": false,

		// true if Rhino globals should be predefined
		"rhino": false,

		// true to allow missing `use strict` pragma
		"sloppy": false,

		// true if blocking ('...Sync') methods can be used.
		"stupid": false,

		// true to allow inefficient subscripting
		"sub": false,

		// true to allow TODO comments
		"todo": false,

		// true to allow unused parameters
		"unparam": false,

		// true to allow more than 1 vars in a function
		"vars": false,

		// true to ignore white-space rules
		"white": false
	},

	// Additional mappings from file extension to language name
	"language.fileExtensions": {},

	// Additional mappings from file name to language name
	"language.fileNames": {},

	// Doing copy and cut when there's no selection will copy or cut the whole lines that have cursors in them
	"lineWiseCopyCut": true,

	// The time in milliseconds after which asynchronous linters time out
	"linting.asyncTimeout": 10000,

	// true to collapse linting panel
	"linting.collapsed": false,

	// true to enable Code Inspection
	"linting.enabled": true,

	// Array of linters to run first
	"linting.prefer": [],

	// true to run providers specified in linting.prefer only
	"linting.usePreferredOnly": false,

	// false to disable live preview reverse inpsect
	"livedev.enableReverseInspect": true,

	// true to enable experimental Live Preview
	"livedev.multibrowser": false,

	// Live Preview Highlight settings
	"livedev.remoteHighlight": {
		"animateEndValue": {
			// Default: rgba(0, 162, 255, 0)
			"background-color": "rgba(0, 162, 255, 0)",

			// Default: 0.6
			"opacity": 0.6
		},

		"animateStartValue": {
			// Default: rgba(0, 162, 255, 0.5)
			"background-color": "rgba(0, 162, 255, 0.5)"
		},

		// Default: rgba(21, 165, 255, 0.85)
		"borderColor": "rgba(21, 165, 255, 0.85)",

		"marginStyling": {
			// Default: rgba(21, 165, 255, 0.58)
			"background-color": "rgba(21, 165, 255, 0.58)"
		},

		"paddingStyling": {
			// Default: rgba(0, 162, 255, 0.5)
			"border-color": "rgba(0, 162, 255, 0.5)",

			// Default: dashed
			"border-style": "dashed",

			// Default: 1px
			"border-width": "1px"
		},

		// Default: true
		"showPaddingMargin": true
	},

	// Port on which WebSocket Server runs for Live Preview
	"livedev.wsPort": 8125,

	// Maximum code hints displayed at once
	"maxCodeHints": 50,

	// Maximum Number of Search Items in Search History
	"maxSearchHistory": 10,

	// 
	"mikaeljorhult.bracketsAutoprefixer.browsers": [],

	// Default: false
	"mikaeljorhult.bracketsAutoprefixer.enabled": false,

	// Default: false
	"mikaeljorhult.bracketsAutoprefixer.onChange": false,

	// Default: false
	"mikaeljorhult.bracketsAutoprefixer.visualCascade": false,

	// true to enable code only mode and hide all other UI elements in Brackets
	"noDistractions": false,

	// true to collapse panes after the last file from the pane is closed via pane header close button
	"pane.mergePanesWhenLastFileClosed": false,

	// Toggle when to show the close and flip-view buttons on the header.
	"pane.showPaneHeaderButtons": "hover",

	// false to disable opening preferences file in split view
	"preferencesView.openPrefsInSplitView": true,

	// false to open user preferences file in left/top pane
	"preferencesView.openUserPrefsInSecondPane": true,

	// The URL of the proxy server used for extension installation
	"proxy": "",

	// true to enable Quick View
	"quickview.enabled": true,

	// true to show image previews for URLs missing extensions
	"quickview.extensionlessImagePreview": true,

	// Enable/disable navigation in recent files
	"recent-files.navigation": true,

	// true to enable scrolling beyond the end of the document
	"scrollPastEnd": false,

	// false to disable all code hints
	"showCodeHints": true,

	// Keeps the blinking cursor visible when you have a text selection
	"showCursorWhenSelecting": false,

	// true to show line numbers in a “gutter” to the left of the code
	"showLineNumbers": true,

	// Automatically indent when creating a new block
	"smartIndent": true,

	// false to turn off soft tabs behavior
	"softTabs": true,

	// true to sort the directories first in the project tree
	"sortDirectoriesFirst": true,

	// Number of spaces to use for space-based indentation
	"spaceUnits": 4,

	// Port number that the built-in server should use for Live Preview
	"staticserver.port": 0,

	// true to highlight background color of the line the cursor is on
	"styleActiveLine": false,

	// Number of spaces to display for tabs
	"tabSize": 4,

	// Select a Brackets theme
	"themes.theme": "light-theme",

	// true to allow custom scroll bars
	"themes.themeScrollbars": true,

	// true to generate uppercase hex colors in Inline Color Editor
	"uppercaseColors": false,

	// true to use tabs instead of spaces
	"useTabChar": false,

	// Wrap lines that exceed the viewport width
	"wordWrap": true
}
